{
  "manifest_version": 2,
  "name": "Website fixes",
  "description": "Some fixes for websites",
  "version": "1.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_title": "Website fixes",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "proxy",
    "tabs",
    "activeTab",
    "<all_urls>",
    "chrome://*/*",
    "background"
  ],
  "web_accessible_resources": [
    "/vendor/jquery.js",
    "/vendor/doT.min.js",
    "/modules/utils.js",
    "/modules/fix_google_search.js",
    "/foreground.js",
    "popup.html"
  ]
}